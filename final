# Repository: DataVizMakers

import matplotlib.pyplot as plt
import numpy as np

# Generate some random data
x = np.linspace(0, 10, 100)
y1 = np.sin(x)
y2 = np.cos(x)

# Plot the data
plt.plot(x, y1, label='Sin')
plt.plot(x, y2, label='Cos')
plt.xlabel('X')
plt.ylabel('Y')
plt.title('Sine and Cosine Curves')
plt.legend()
plt.show()
